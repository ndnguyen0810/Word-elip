package Models;

public class ThucAn {
	
	//Constractor
	private int maTA;
	private String tenTA;
	private int gia;
	private String dVT;
	private int maLoai;
	
	public ThucAn(int maTA, String tenTA, int gia, String dVT, int maLoai) {
		super();
		this.maTA = maTA;
		this.tenTA = tenTA;
		this.gia = gia;
		this.dVT = dVT;
		this.maLoai = maLoai;
	}

	public ThucAn() {
		super();
	}

	public int getMaTA() {
		return maTA;
	}

	public void setMaTA(int maTA) {
		this.maTA = maTA;
	}

	public String getTenTA() {
		return tenTA;
	}

	public void setTenTA(String tenTA) {
		this.tenTA = tenTA;
	}

	public int getGia() {
		return gia;
	}

	public void setGia(int gia) {
		this.gia = gia;
	}

	public String getdVT() {
		return dVT;
	}

	public void setdVT(String dVT) {
		this.dVT = dVT;
	}

	public int getMaLoai() {
		return maLoai;
	}

	public void setMaLoai(int maLoai) {
		this.maLoai = maLoai;
	}
	// xử lý dữ liệu
}
