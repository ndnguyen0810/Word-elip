package View.CaiDat;

import javax.swing.JPanel;
import java.awt.BorderLayout;
import javax.swing.BoxLayout;
import javax.swing.JButton;

public class JP_CaiDat extends JPanel {

	/**
	 * Create the panel.
	 */
	public JP_CaiDat() {
		setLayout(new BoxLayout(this, BoxLayout.X_AXIS));
		
		JPanel pnMain = new JPanel();
		add(pnMain);
		pnMain.setLayout(new BorderLayout(0, 0));
		
		JPanel pnLeft = new JPanel();
		pnMain.add(pnLeft, BorderLayout.WEST);
		pnLeft.setLayout(new BorderLayout(0, 0));
		
		JPanel pnButton = new JPanel();
		pnLeft.add(pnButton, BorderLayout.NORTH);
		pnButton.setLayout(new BoxLayout(pnButton, BoxLayout.PAGE_AXIS));
		
		JButton btnNewButton = new JButton("New button");
		pnButton.add(btnNewButton);
		
		JButton btnNewButton_1 = new JButton("New button");
		pnButton.add(btnNewButton_1);
		
		JPanel pnCenter = new JPanel();
		pnMain.add(pnCenter, BorderLayout.CENTER);
		
		JPanel pnAdd = new JPanel();
		pnCenter.add(pnAdd);

	}

}
